<!-- this edit.html.erb file is to allow for the -->

<h1>Edit Picture</h1>


<%= link_to "Back", doggypics_url %>
<%= form_for doggypics_path(@doggie) do |f| %>
    <p>
      <%= f.label "owner" %><br>
      <%= f.text_field "owner" %>
    </p>
    <p>
      <%= f.label "dog_name" %><br>
      <%= f.text_field "Dog Name" %>
    </p>
    <p>
      <%= f.label "dog_breed" %><br>
      <%= f.text_field "Dog Breed" %>
    </p>
    <p>
      <%= f.label "url" %><br>
      <%= f.text_field "url" %>
    </p>
    <p>
      <%= f.submit "Save" %>
    </p>
<% end %>